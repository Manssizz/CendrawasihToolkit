{
    "name": "TorToolkit-Telegram",
    "description": "Telegram leecher bot with python",
    "logo": "https://raw.githubusercontent.com/devillD/TorToolkit-Telegram/heroku/toolkit.jpg",
    "keywords": [
      "telegram",
      "bot",
      "qBittorrent",
      "python"
    ],
    "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "13"
        }
      }
    ],
    "repository": "https://github.com/devillD/TorToolkit-Telegram",
    "website": "https://github.com/devillD/TorToolkit-Telegram/tree/heroku",
    "success_url": "https://github.com/devillD/TorToolkit-Telegram/blob/heroku/readme.md",
    "stack": "container",
    "env": {
    "API_HASH": {
      "description": "Get your api hash from http://my.telegram.org/",
      "value": ""
    },
    "API_ID": {
      "description": "Get your api id from http://my.telegram.org/",
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Get your bot token from @BotFather",
      "value": ""
    },
    "BASE_URL": {
      "description": "Base URL for selecting torrent file",
      "value": "<your app name>.herokuapp.com"
    },
    "RCLONE_DRIVE": {
      "description": "Paste your RClone config here",
      "value": "",
      "required" : false
    },
    "CMD_POSTFIX": {
      "description": "set True/False for using tag bot i.e: /leech or /leech@CendrawasihToolkit",
      "value": "True",
      "required" : false
    },
    "MEGA_ENABLE": {
      "description": "set True/False for enabling Mega support",
      "value": "False",
      "required" : false
    },
    "MEGA_API": {
      "description": "Fill with your Mega api key if you enabling Mega.nz support",
      "value": "",
      "required" : false
    },
    "MEGA_UNAME": {
      "description": "Fill with your Mega username key if you enabling Mega.nz support",
      "value": "",
      "required" : false
    },
    "MEGA_PASS": {
      "description": "Fill with your Mega password key if you enabling Mega.nz support",
      "value": "",
      "required" : false
    },
    "STATUS_DEL": {
      "description": "Auto delete message in seconds. Default 5",
      "value": "",
      "required" : false
    },
    "LEECH_CMD": {
      "description": "Leech command",
      "value": "leech"
    },
    "PURGE_CMD": {
      "description": "Purge command",
      "value": "purge"
    },
    "PAUSEALL_CMD": {
      "description": "Pauseall command",
      "value": "pauseall"
    },
    "RESUMEALL_CMD": {
      "description": "Resumeall command",
      "value": "resumeall"
    },
    "STATUS_CMD": {
      "description": "Status command",
      "value": "status"
    },
    "SETTINGS_CMD": {
      "description": "Admin Settings command! Better not keep it as default. Make it harder to guess ;)",
      "value": "settings"
    },
    "EXEC_CMD": {
      "description": "execute command",
      "value": "exec"
    },
    "UPLOAD_CMD": {
      "description": "Upload command",
      "value": "upload"
    },
    "YTDL": {
      "description": "Youtube Download command",
      "value": "ytdl"
    },
    "PYTDL": {
      "description": "PlayList of youtube download command",
      "value": "pytdl"
    },
    "ABOUT_CMD": {
      "description": "About command",
      "value": "about"
    },
    "GETLOGS_CMD": {
      "description": "GetLog command",
      "value": "getlogs"
    },
    "SERVER_CMD": {
      "description": "Get server info command",
      "value": "server"
    },
    "USERSETTINGS_CMD": {
      "description": "User Settings command for specified users or groups",
      "value": "uset"
    }
  }
}